<Window x:Class="MOSC.SettingService"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MOSC"
        mc:Ignorable="d"
        Title="Настройка службы MOSK" Height="700" Width="770" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="183*"/>
            <ColumnDefinition Width="581*"/>
        </Grid.ColumnDefinitions>
        <StackPanel>
            <Button x:Name="btnGeneral" Content="Общее" FontSize="16" Height="50" Click="btnGeneral_Click"/>
            <Button x:Name="btnSchedule" Content="Расписание" FontSize="16" Height="50" Click="btnSchedule_Click"/>
            <Button x:Name="btnTask" Content="Задачи" FontSize="16" Height="50" Click="btnTask_Click"/>
            <Button x:Name="btnResources" Content="Ресурсы" FontSize="16" Height="50" Click="btnResources_Click"/>
        </StackPanel>

        <Frame x:Name="pageContent" Grid.Column="1" Margin="10" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
